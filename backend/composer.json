{
    "name": "mixtend/calendar-backend",
    "type": "project",
    "description": "MIXTEND Calendar Backend API using Flight",
    "keywords": ["flight", "api", "calendar"],
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "mikecao/flight": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
